<p id="notice"><%= notice %></p>

<h1>Listing Haikus</h1>

<table>
  <thead>
    <tr>
      <th>Haiku</th>
      <th>Likes</th>
      <th>Compgen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @haikus.each do |haiku| %>
      <tr>
        <td>
          <%= render partial: 'show', locals:{haiku: haiku} %>
        </td>
        <td><%= haiku.likes %></td>
        <td><%= haiku.compGen %></td>
      </tr>
    <% end %>
  </tbody>
</table>
